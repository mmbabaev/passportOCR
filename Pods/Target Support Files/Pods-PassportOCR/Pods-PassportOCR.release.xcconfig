CLANG_CXX_LIBRARY = compiler-default
CLANG_MODULES_AUTOLINK = YES
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$CONFIGURATION_BUILD_DIR/GPUImage.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/MRProgress.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/TesseractOCR.framework/Headers" -iquote "$CONFIGURATION_BUILD_DIR/UIImage_Resize.framework/Headers"
OTHER_LDFLAGS = $(inherited) -ObjC -weak_library /usr/lib/libstdc++.6.0.9.dylib -l"stdc++" -framework "GPUImage" -framework "MRProgress" -framework "TesseractOCR" -framework "UIImage_Resize"
PODS_FRAMEWORK_BUILD_PATH = $(BUILD_DIR)/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)/Pods-PassportOCR
PODS_ROOT = ${SRCROOT}/Pods